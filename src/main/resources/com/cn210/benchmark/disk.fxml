<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cn210.benchmark.page.DiskController">
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false">
                    <graphic>
                        <Label onMouseClicked="#onHomeButtonClick" text="Home" />
                    </graphic>
                </Menu>
                <Menu mnemonicParsing="false">
                    <graphic>
                        <Label onMouseClicked="#onSystemButtonClick" text="System" />
                    </graphic>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onAboutButtonClick" text="About" />
                        <MenuItem mnemonicParsing="false" onAction="#onSRCButtonClick" text="Source Code" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <VBox alignment="CENTER" prefHeight="375.0" prefWidth="600.0">
            <children>
                <Button fx:id="StartDiskBenchmark" mnemonicParsing="false" onAction="#onStartDiskButtonClick" text="Start Disk Benchmark">
                    <font>
                        <Font name="System Bold" size="24.0" />
                    </font>
                </Button>
                <VBox fx:id="DiskProcessing" prefHeight="400.0" prefWidth="600.0" visible="false">
                    <children>
                        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="100.0">
                            <children>
                                <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../../loading.gif" />
                                    </image>
                                </ImageView>
                            </children>
                        </VBox>
                        <HBox prefHeight="50.0" prefWidth="600.0">
                            <children>
                                <Label text="Time:">
                                    <font>
                                        <Font name="System Bold" size="18.0" />
                                    </font>
                                    <HBox.margin>
                                        <Insets left="5.0" />
                                    </HBox.margin>
                                </Label>
                                <Label fx:id="DiskTimeBenchmark" text="0">
                                    <font>
                                        <Font size="18.0" />
                                    </font>
                                    <HBox.margin>
                                        <Insets left="3.0" />
                                    </HBox.margin>
                                </Label>
                            </children>
                        </HBox>
                    </children></VBox>
                <VBox fx:id="DiskFinished" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" visible="false">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Label text="Score:">
                                    <font>
                                        <Font name="System Bold" size="40.0" />
                                    </font>
                                </Label>
                                <Label fx:id="DiskScore" text="DISK_SCORE">
                                    <font>
                                        <Font size="40.0" />
                                    </font>
                                    <HBox.margin>
                                        <Insets left="10.0" />
                                    </HBox.margin>
                                </Label>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Label text="Time:">
                                    <font>
                                        <Font name="System Bold" size="30.0" />
                                    </font>
                                </Label>
                                <Label fx:id="DiskTime" text="DISK_TIME">
                                    <font>
                                        <Font size="30.0" />
                                    </font>
                                    <HBox.margin>
                                        <Insets left="10.0" />
                                    </HBox.margin>
                                </Label>
                            </children>
                        </HBox>
                        <Button mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
                            <font>
                                <Font size="20.0" />
                            </font>
                        </Button>
                    </children></VBox>
            </children></VBox>
    </children>
</VBox>
